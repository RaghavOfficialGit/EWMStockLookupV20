{
  "welcomeFile": "/index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/stock/(.*)$",
      "target": "/stock/$1",
      "destination": "srv-api",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^(.*)$",
      "target": "$1",
      "localDir": "stock-lookup/webapp",
      "authenticationType": "xsuaa"
    }
  ]
}
